{
  "name": "frogstatus",
  "version": "1.0.0",
  "private": true,
  "description": "FrogStatus - JFrog demo application",
  "workspaces": [
    "app-api",
    "app-ui"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=app-api & npm run dev --workspace=app-ui",
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present",
    "set-deps-state": "tsx scripts/set-deps-state.ts",
    "set-vulnerable": "npm run set-deps-state vulnerable",
    "set-clean": "npm run set-deps-state clean"
  },
  "devDependencies": {
    "tsx": "^4.7.0"
  },
  "overrides": {
    "@hono/node-server": "1.19.7",
    "baseline-browser-mapping": "2.9.13",
    "@types/node": "22.0.0",
    "hono": "4.6.11",
    "@types/express-serve-static-core": "4.19.6",
    "@babel/code-frame": "7.24.7",
    "@babel/compat-data": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/generator": "7.24.7",
    "@babel/helper-compilation-targets": "7.24.7",
    "@babel/helper-module-imports": "7.24.7",
    "@babel/helper-module-transforms": "7.24.7",
    "@babel/helper-plugin-utils": "7.24.8",
    "@babel/helpers": "7.24.7",
    "@babel/parser": "7.24.7",
    "@babel/plugin-syntax-import-attributes": "7.24.7",
    "@babel/plugin-syntax-jsx": "7.24.7",
    "@babel/plugin-syntax-typescript": "7.24.7",
    "@babel/runtime": "7.24.7",
    "@babel/template": "7.24.7",
    "@babel/traverse": "7.24.7",
    "@babel/types": "7.24.7",
    "caniuse-lite": "1.0.30001640",
    "which-typed-array": "1.1.15",
    "node-abi": "3.65.0",
    "ufo": "1.5.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
